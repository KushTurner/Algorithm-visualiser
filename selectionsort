import random

mylist = [*range(1, 6)]
random.shuffle(mylist)


def selectionsort(arr):
    print(arr)
    for i in range(len(arr)):
        cur_min_idx = i
        for j in range(i+1, len(arr)):
            if arr[j] < arr[cur_min_idx]:
                cur_min_idx = j
        
        arr[i], arr[cur_min_idx] = arr[cur_min_idx], arr[i]
        print(arr)

selectionsort(mylist)

